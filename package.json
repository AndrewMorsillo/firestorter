{
	"name": "firestorter",
	"version": "0.16.1",
	"description": "Simple & super fast Firestore to React bindings using Mobx observables",
	"keywords": [
		"react-firestore-mobox",
		"react-firestore",
		"react-firebase",
		"firestorter",
		"firestoreter",
		"firestore-mobx",
		"firebase-mobx",
		"firestore-react",
		"mobx-firestore",
		"firestore-collection"
	],
	"main": "dist/firestorter.js",
	"module": "dist/firestorter.module.js",
	"react-native": "src/index.js",
	"repository": "https://github.com/IjzerenHein/firestorter",
	"author": "IjzerenHein <hrutjes@gmail.com>",
	"license": "MIT",
	"files": [
		"src",
		"dist"
	],
	"scripts": {
		"build": "tsc",
		"lint": "tslint -p tsconfig.json",
		"format": "prettier --write \"src/**/*.ts\" \"src/**/*.js\"",
		"test": "node ./test/seedSampleData.js && jest",
		"cov": "jest --coverage",
		"doc": "documentation build src/** -f md > ./docs/API.md"
	},
	"dependencies": {
		"lodash.isequal": "^4.5.0"
	},
	"peerDependencies": {
		"mobx": "^4.1.0"
	},
	"devDependencies": {
		"@firebase/app-types": "^0.3.2",
		"@firebase/firestore-types": "^0.7.0",
		"@release-notes/cli": "^0.3.0",
		"@types/jest": "^23.3.2",
		"babel-eslint": "^10.0.1",
		"babel-plugin-transform-flow-strip-types": "^6.22.0",
		"babel-preset-es2015-rollup": "^3.0.0",
		"codecov": "^3.1.0",
		"documentation": "^8.1.2",
		"firebase": "^5.5.2",
		"jest": "^23.6.0",
		"mobx": "^5.0.2",
		"prettier": "^1.14.3",
		"superstruct": "^0.6.0",
		"ts-jest": "^23.10.3",
		"tslint": "^5.11.0",
		"tslint-config-prettier": "^1.15.0",
		"typescript": "^3.1.1"
	}
}
